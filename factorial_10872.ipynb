   "source": [
    "# 백준코딩 10872 재귀함수를 이용한 factorial 반환\n",
    "N = int(input())\n",
    "def factorial(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "\n",
    "    return n * factorial(n-1)\n",
    "\n",
    "print(factorial(N))"
   ]
  }
